{
  "mcpServers": {
    "github": {
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "$GITHUB_ACCESS_TOKEN_MCPHUB"
      },
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "disabled_tools": ["manage_notification_subscription"],
      "name": "github",
      "command": "docker",
      "disabled": false
    }
  },
  "nativeMCPServers": []
}

